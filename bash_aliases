alias open='emacs -nw'
alias sopen='sudo emacs -nw'
alias tclean='rm #*# | rm *~ | rm .#*'
alias updateAliases='cp ~/.bash_aliases ~/'
alias showAliases='echo ~/.bash_aliases'
alias merge='git merge'
alias whereB='git branch -av'
alias push='git push'
alias commit='git commit'
alias add='git add'
alias checkOut='git checkout'
alias GG='sudo shutdown -h now'	